  "description": "Doctrine Async Bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.17 || ^3.0",
    "symfony/framework-bundle": "^6.4",
    "tourze/doctrine-entity-checker-bundle": "0.0.*",
    "tourze/symfony-async-bundle": "0.0.*",
    "psr/log": "^3|^2|^1",
